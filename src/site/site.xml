<?xml version="1.0" encoding="ISO-8859-1"?>
<project xmlns="http://maven.apache.org/DECORATION/1.4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.4.0 http://maven.apache.org/xsd/decoration-1.4.0.xsd" name="Shabdiz">
    <skin>
        <groupId>lt.velykis.maven.skins</groupId>
        <artifactId>reflow-maven-skin</artifactId>
        <version>1.0.0</version>
    </skin>
    <publishDate position="bottom" format="dd MMM. yyyy"/>
    <version position="bottom"/>
    <custom>
        <reflowSkin>
            <highlightJs>true</highlightJs>
            <highlightJsTheme>github</highlightJsTheme>
            <bottomNav maxSpan="10">
                <column>reports</column>
                <column>modules</column>
                <column>parent</column>
            </bottomNav>
            <brand>
                <name>Shabdiz</name>
                <href>index.html</href>
            </brand>
            <navbarInverse>true</navbarInverse>
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <breadcrumbs>true</breadcrumbs>

            <skinAttribution>false</skinAttribution>
        </reflowSkin>
    </custom>

    <body>
        <links>
            <item name="Javadoc" href="apidocs"/>
        </links>

        <menu ref="modules"/>
        <menu name="Maven documentation" ref="reports" inherit="bottom"/>
        <menu ref="parent"/>
    </body>
</project>
